{
  "name": "react-node-isomorphic-minimal",
  "version": "0.0.1",
  "description": "Minimal isomorphic react, node app skeleton",
  "main": "index.js",
  "author": "Stephen Pearce (https://github.com/pearces)",
  "license": "MIT",
  "keywords": [
    "react",
    "node",
    "express",
    "webpack"
  ],
  "engines": {
    "node": ">=12.10.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.36.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.7",
    "null-loader": "^4.0.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "webpack-cli": "^4.6.0",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "build": "webpack --progress --mode production",
    "build:dev": "NODE_ENV=development webpack --progress --mode development",
    "clean": "node clean.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "NODE_ENV=production node dist/server.js",
    "server:dev": "sleep 12; NODE_ENV=development nodemon --delay 2.5 --watch dist --ext js --ignore dist/bundle.js dist/server.js",
    "start": "npm run clean && npm run build && npm run server",
    "start:dev": "npm run clean && concurrently \"npm:build:dev\" \"npm:server:dev\"",
    "lint": "eslint src"
  }
}
